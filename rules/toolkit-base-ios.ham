#----------------------------------------------------------------------
#     LOA
#----------------------------------------------------------------------
LOA_OS = ios ;
LOA_LINKER = gcc ;
LOA_ARCH = $(IOS_CPUSET:L) ;
DEFINES += _COMPILER_IOS ;

#----------------------------------------------------------------------
#     DEFINES
#----------------------------------------------------------------------
DEFINES += niPosix IOS ;

#----------------------------------------------------------------------
#     OS Variables
#----------------------------------------------------------------------
POSIX = 1 ;
IOS = 1 ;
EMBEDDED = 1 ;
NO_PCH = 1 ;
NO_DLL = 1 ;
BUILD_JNI = 0 ;
